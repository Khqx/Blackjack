    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>Resources\Icons\blackjack.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Localization\translations_all.csv" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\Icons\blackjack.ico" />
