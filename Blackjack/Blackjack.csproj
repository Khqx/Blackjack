  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\Icons\blackjack.ico" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Resources\Images\" />
    <Folder Include="UI\" />
  </ItemGroup>

</Project>